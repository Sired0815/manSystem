* {
    margin: 0;
    padding: 0;
}
.fl {
    float: right;
    font-weight: bolder;
    text-indent: 0.5em;
    font-size: 15px;
    color: #f5fffa;
    margin-right: 20px;
    margin-top: 22px;
    text-align: center;
    /* css注释： 设置margin为对象上下间距10px */

}

.right {
    text-align: center;
    position: relative;
    z-index: 0;
    margin-top: 460px;
    font-weight: bolder;
    height: 50px;
    border: 1px solid rgba(67, 249, 209, 0.08);
    color: #7dbad7;

}

ul {
    list-style: none;
}


body {
    font: 12px/1.2em Microsoft YaHei;
}

h2 {

    font-family: 华文宋体;
    margin-top: 32px;
    position: relative;
    font-size: 25px;
    cursor: pointer;
    color: white;


}

.superShadow {
    text-shadow: 0 1px 0 hsl(174, 5%, 80%),
    0 2px 0 hsl(174, 5%, 75%),
    0 3px 0 hsl(174, 5%, 70%),
    0 4px 0 hsl(174, 5%, 66%),
    0 5px 0 hsl(174, 5%, 64%),
    0 6px 0 hsl(174, 5%, 62%),
    0 7px 0 hsl(174, 5%, 61%),
    0 8px 0 hsl(174, 5%, 60%),
    0 0 5px rgba(0, 0, 0, .05),
    0 1px 3px rgba(0, 0, 0, .2),
    0 3px 5px rgba(0, 0, 0, .2),
    0 5px 10px rgba(0, 0, 0, .2),
    0 10px 10px rgba(0, 0, 0, .2),
    0 20px 20px rgba(0, 0, 0, .3);
}

/*对下拉框里的文字去除样式*/
a,
a:link {
    color: #000;
    text-decoration: none;
    outline: none;
}

a:hover,
a:focus {
    text-decoration: none;

}

label,
button,
input,
select,
textarea {
    font-size: 12px;
    vertical-align: middle;
}

input,
button,
select,
textarea {
    font: 12px/1.2em Microsoft YaHei;
}


.tabs-tool {
    border-top: 0;
}

.text,
.textarea {
    padding: 3px;

    width: 260px;
}

.text {
    height: 14px;
    line-height: 14px;
}

.header {
    height: 90px;
    position: relative;
    z-index: 0;
    overflow: hidden;

    background: url(../img/header.jpg) bottom repeat-x;

}

.header-left {
    position: absolute;
    z-index: 1;
    left: 15px;
    top: 0;
}

.header-right {
    position: absolute;
    z-index: 1;
    right: 5px;
    top: 0;
    color: #fff;
    text-align: right;
}

.header-right p {
    line-height: 0.7em;
}

.header-right a {
    color: #fff;
    margin: 0 5px;
}

.sidebar {
    width: 160px;
}

.side-tree .tree-node {
    padding: 3px 0px;
}

.side-tree a {
    display: block;
}




